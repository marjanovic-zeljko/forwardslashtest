#{{sectionID}} img.ux-image {
  width: 100%;
    {% if sirinaSlike == 'square' %}
  width: 200px;
  aspect-ratio: 1;

  {% elsif sirinaSlike == 'portrait' %}
  width: 100px;
  height:200px;
  object-fit: scale-down;
      {% elsif sirinaSlike == 'landscape' %}
  width: 200px;
  height:100px;
  object-fit: cover;
      {% endif %}
  background-size: contain!important; 
  background-repeat: no-repeat!important;
  background-position: center!important;
}
        
    #{{sectionID}} .swiper-slide.card .UXfeature-row__text-button .ux-btn {  
    
    {% if debljinaFontaDugmeMobilni != "default" %} 
      font-weight:{{ debljinaFontaDugmeMobilni }} !important;
    {%endif%} 
    
      
      color:{{ dugmeBojatekstaMobilni }};
      background-color:{{ dugmeBojaPozadineMobilni }}; 
      
       {% if dugmeBorderMobilni != '0px' %} 
         border: {{ dugmeBorderMobilni }}px solid {{ dugmeBorderBojaMobilni }}; 
       {% endif %} 
      
         border-radius:{{ dugmeBorderRadiusMobilni }}px; 
         padding:{{ dugmePadingVertikalniMobilni }}px {{ dugmePadingHorizontalniMobilni }}px; 
         margin-bottom:0!important; line-height:initial!important; 
    
      {% if DugmeVelicinaFontaMobilni != '0px' %}
          font-size:{{ DugmeVelicinaFontaMobilni }}px;
        {% endif %}
        
           {% if dugmeRazmakIzmedjuSlova != '0px' %}
             letter-spacing:{{ dugmeRazmakIzmedjuSlovaMobilni }}px;
          {% endif %} 
    } 
        
    #{{sectionID}} .swiper-slide.card .UXfeature-row__text-button .ux-btn:hover {
        color:{{ dugmeBojatekstaHoverMobilni }}; 
        background-color:{{ dugmeBojaPozadineHoverMobilni }};
        {% if dugmeBorderMobilni != '0' %}
        border-color:{{ dugmeBorderBojaHoverMobilni }};
        {% endif %} 
    }
         
           {% if section.settings.section_button_alignment == 'left' %}
           #{{sectionID}} .swiper-slide.card .UXfeature-row__text-button {
             align-self: start;
           } 
             {% elsif section.settings.section_button_alignment == 'center' %}
             #{{sectionID}} .swiper-slide.card .UXfeature-row__text-button {
             align-self: center;
           }
             {% elsif section.settings.section_button_alignment == 'right' %}
             #{{sectionID}} .swiper-slide.card .UXfeature-row__text-button {
             align-self: end;
           }
             {% endif %}
    
      
    
    #{{sectionID}} .swiper-slide.card .UXfeature-row__text-button a {
      text-decoration: none;
    }







#{{ sectionID}} {
    margin: {{ podesavanjegornjemarginesekcijacelaMobilni}}px auto {{ podesavanjedonjemarginesekcijacelaMobilni}}px auto ;
  padding: {{ podesavanjegornjegpadingacelasekcijaMobilni}}px {{ horizontalnipadingcelasekcijaMobilni}}px {{ podesavanjedonjegpadingacelasekcijaMobilni}}px; 

}  

#{{sectionID}} section .carouselTitles {
display: flex;
width: 100%;
flex-wrap: wrap;
flex-direction: column;
align-content: center;
}

#{{sectionID}} {
width: {{maksSirinaKontentaMobilni}}%;
max-width: 100%;
}



#{{sectionID}} .card .card-content{
display: flex;
flex-direction: column;
align-items: center;
position: relative;
z-index: 100;
height: 100%;
}


#{{sectionID}} div.ux-image {
width: 100%;
height: {{sirinaSlikeMobilni}}%;
max-height: 200px;
background-size: contain!important; 
background-repeat: no-repeat!important;
background-position: center!important;}

}

#{{sectionID}}  .ux-text, #{{sectionID}}  .ux-button {
--duzinatextidugme: calc(400px - {{sirinaSlike}}%);
height: calc(var(--duzinatextidugme)/2);
}

#{{sectionID}} section .card .ux-image img{
width: {{sirinaSlike}};
border: 3px solid #fff;
} 

#{{sectionID}} .card .ux-text{
display: flex;
flex-direction: column;
align-items: center;
padding-top: 20px;
color: black;
} 

#{{sectionID}} .ux-text .heading{
font-size: 20px;
font-weight: 600;
}

#{{sectionID}} .card .ux-button{
display: flex;
justify-content: center;
padding-top: 20px;
flex-direction: column;

}

#{{sectionID}} .card .ux-button button{
background: #7d2ae8;
outline: none;
border: none;
color: #fff;
padding: 8px 22px;
border-radius: 10px;
font-size: 14px;
transition: all 0.3s ease;
cursor: pointer;
}

#{{sectionID}} .ux-button button:hover{
background: #6616d0;
}

#{{sectionID}} .swiper-pagination{
position: absolute;
top: 450px;
}

#{{sectionID}} .swiper-pagination-bullet{
height: 7px;
width: 26px;
border-radius: 25px;
background: black;
}

        }
    
       @media only screen and (max-width: 769px) and (min-width: 1200px) {
       #{{sectionID}} .carouselTitles .section-title, #{{sectionID}} .subheadline, #{{sectionID}} .carouselTitles .UX-custom-paragraph  {
      min-width:400px!important;
    }}
    @media only screen and (max-width: 1200px) and (min-width: 1600px) {
       #{{sectionID}} .carouselTitles .section-title, #{{sectionID}} .carouselTitles .subheadline, #{{sectionID}} .carouselTitles .UX-custom-paragraph {
      min-width: 500px!important;
}}
