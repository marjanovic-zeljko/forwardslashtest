  outline: none;
  border: none;
  color: #fff;
  padding: 8px 22px;
  border-radius: 10px;
  font-size: 14px;
  transition: all 0.3s ease;
  cursor: pointer;
}

#{{sectionID}} .ux-button button:hover{
  background: #6616d0;
}

#{{sectionID}} .swiper-pagination{
  position: absolute;
  top: 450px;
}

