{% schema %}
{
   "name":"EmbedUX - Carousel",
   "tag":"section",
   "class":"spaced-section",
   "settings":[
      {
         "type":"header",
         "content":"IMPORTANT INFORMATION",
         "info":"All possible customizations for an element are marked with the same number (e.g. [1]). Making it easier to identify the options that are relevant to the same element."
      },
      {
         "type":"header",
         "content":"SECTION STYLING - [1]"
      },
      {
         "type":"color",
         "id":"section_background_color",
         "label":"Section Background Color - [1]"
      },
      {
         "type":"color",
         "id":"background_color",
         "label":"Card Background Color - [1]"
      },
      {
         "type":"range",
         "id":"content_max_width",
         "min":50,
         "max":100,
         "step":10,
         "unit":"%",
         "label":"Section Max Width - [1]",
         "default":100
      },
      {
         "type":"range",
         "id":"section_margin_top",
         "min":0,
         "max":200,
         "step":5,
         "unit":"px",
         "label":"Section Top Margin - [1]",
         "default":20
      },
      {
         "type":"range",
         "id":"section_margin_bottom",
         "min":0,
         "max":200,
         "step":5,
         "unit":"px",
         "label":"Section Bottom Margin - [1]",
         "default":20
      },
      {
         "type":"range",
         "id":"section_padding_top",
         "min":0,
         "max":100,
         "step":5,
         "unit":"px",
         "label":"Section Top Padding - [1]",
         "default":50
      },
      {
