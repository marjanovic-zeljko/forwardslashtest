<section>
 <div class="carouselTitles">
       <div>
       {%- if preduslovzaHelement != blank -%}
          <{{velicinaHelementa}} class="section-title">{{preduslovzaHelement}}</{{velicinaHelementa}}>
        {%- endif -%}
        </div>
            <div class="subheadlineContainer">
       {% if preduslovzaHelementSubheadline != blank %}
                <{{velicinaHelementaSubheadline}} class="subheadline">{{preduslovzaHelementSubheadline}}</{{velicinaHelementaSubheadline}}>
            {% endif %}
            </div>
        {%- if preduslovzaPelement != blank -%}
        <div class="UX-custom-paragraph">
                    {{preduslovzaPelement}}
                </div>
      {%- endif -%}
                </div>

<div id="mySwiper" class="swiper mySwiper container"
data-cardNumber="{{kolicinaKartica}}"
data-autoPlay="{{ autoPlej }}"
data-speed="{{ brzinaSlidera }}"
data-card-space="{{ razmakIzmenjuKartica }}"
data-swiper-infinitee="{{ neogranicenSkrol }}"> 
 <div class="swiper-wrapper content">
{% for block in section.blocks %}
 {% assign dugmePocetniTekstKartica = block.settings.cardbutton_label %}
 {% assign dugmeLinkKartica = block.settings.cardbutton_url  %}
 {% assign preduslovzaHelementKartica = block.settings.cardsection_title %}
 {%  assign preduslovzaPelementKartica = block.settings.cardsection_textblock %}
  {% assign urlSlike = block.settings.urlSlike  %}
 
  
  {%- case block.type -%}
   {%- when 'contentBlock' -%}

        <div class="swiper-slide card">
          <div class="card-content">
     {%- assign urlSlike =  urlSlike | img_url: 'master' -%}
             <img class="ux-image" src="{{ urlSlike }}"/>
            <div class="ux-text">
              {%- if preduslovzaHelementKartica != blank -%}
                <{{velicinaHelementaKartica}} class="section-title-card">{{preduslovzaHelementKartica}}</{{velicinaHelementaKartica}}>
              {%- endif -%}
              {%- if preduslovzaPelementKartica != blank -%}
                <div class="UX-custom-paragraph-card">
                    {{preduslovzaPelementKartica}}
                </div>
              {%- endif -%}
            </div>
             {%- if dugmePocetniTekstKartica != blank -%}
              <div class="UXfeature-row__text-button ux-button">
               <a href="{{ dugmeLinkKartica }}" class="ux-btn">
                {{ dugmePocetniTekstKartica }}
               </a>         
              </div>
             {% endif %}
          </div>
        </div>
  
{% endcase %}
{% endfor %}

   

</div>
 </div>
        <div class="swiper-button-next middle nxt"></div>
      <div class="swiper-button-prev middle"></div>
<!--       <div class="swiper-pagination"></div>            -->
         <div class="bottom-button_container">         
      <div class="swiper-button-prev bottom"></div>
      <div class="swiper-button-next bottom nxt"></div>
                </div>
  </section>
