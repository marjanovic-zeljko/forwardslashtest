/*STYLE FOR STOCK CAROUSEL*/
*{
    margin: 0;
    padding: 0;
  }

#{{sectionID}} section{
  position: relative;  
  width: 100%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
 
  
@media only screen and (min-width:768px){
 #{{ sectionID }} {
      margin: {{ podesavanjegornjemarginesekcijacela }}px auto {{ podesavanjedonjemarginesekcijacela }}px auto;
    padding: {{ podesavanjegornjegpadingacelasekcija }}px {{ horizontalnipadingcelasekcija }}px {{ podesavanjedonjegpadingacelasekcija }}px; 
  
}  

#{{sectionID}} section .carouselTitles {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  flex-direction: column;
  align-content: center;
}

#{{sectionID}} {
  width: {{maksSirinaKontenta}}%;
  max-width: 100%;
}



#{{sectionID}} .card .card-content{
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  z-index: 100;
  height: 100%;
}


#{{sectionID}} img.ux-image {
  width: 100%;
    {% if sirinaSlike == 'square' %}
  width: 200px;
  aspect-ratio: 1;

  {% elsif sirinaSlike == 'portrait' %}
  width: 100px;
  height:200px;
  object-fit: scale-down;
      {% elsif sirinaSlike == 'landscape' %}
  width: 200px;
  height:100px;
  object-fit: cover;
      {% endif %}
  background-size: contain!important; 
  background-repeat: no-repeat!important;
  background-position: center!important;
}
  
}

  #{{sectionID}}  .ux-text, #{{sectionID}}  .ux-button {
  --duzinatextidugme: calc(400px - {{sirinaSlike}}%);
  height: calc(var(--duzinatextidugme)/2);
}



#{{sectionID}} .card .ux-text{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 20px;
  color: black;
} 

#{{sectionID}} .ux-text .heading{
  font-size: 20px;
  font-weight: 600;
}

#{{sectionID}} .card .ux-button{
  display: flex;
  justify-content: center;
  padding-top: 20px;
  flex-direction: column;

}

