 {% if pozicijaStrelicaMobilni == "center" %}
#{{sectionID}} .swiper-button-next.middle, .swiper-button-prev.middle{
  opacity: 0.7;
  color: {{ bojaStrelicaMobilni }};
  transition: all 0.3s ease;
  display: inline-flex;
 
}
   #{{sectionID}} .swiper-button-next.middle::after, .swiper-button-prev.middle::after{
